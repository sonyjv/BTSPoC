        </xs:simpleType>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="BookingAmt" type="v1:Amount"/>
      <xs:element maxOccurs="1" minOccurs="0" name="TrxPrice" type="v1:Amount"/>
      <xs:element maxOccurs="1" minOccurs="0" name="ExecTime">
